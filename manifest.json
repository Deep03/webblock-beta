{
  "manifest_version": 3,
  "name": "Web Block",
  "version": "1.0",
  "description": "Block websites from being accessed",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icons8-block-16.png",
    "48": "icons/icons8-block-48.png",
    "128": "icons/icons8-block-128.png"
  },
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
   "*://*/*"
  ],
  "action": {
    "default_icon": "icons/icons8-block-128.png",
    "default_title": "WebBlock-TEST",
    "default_popup": "index.html"
  }
}
